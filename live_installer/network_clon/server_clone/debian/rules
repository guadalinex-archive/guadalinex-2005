#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

include /usr/share/cdbs/1/rules/debhelper.mk

pre-build::
	chmod +x server-clone  server-clone-start
	chmod +x hooks/*
	for x in `find clone-scripts/ -maxdepth 1 -type d | tail -n+2`; do \
		chmod -R +x $$x; \
	done
