Depends: samba-common (= 3.0.14a-6ubuntu1), netbase, logrotate, libacl1 (>= 2.2.11-1), libc6 (>= 2.3.4-1), libcomerr2 (>= 1.33-3), libcupsys2-gnutls10 (>= 1.1.23-1), libkrb53 (>= 1.3.2), libldap2 (>= 2.1.17-1), libpam0g (>= 0.76), libpopt0 (>= 1.7), debconf (>= 0.5) | debconf-2.0, libpam-runtime (>= 0.76-13.1), libpam-modules, lsb-base (>= 1.3-9ubuntu3)
