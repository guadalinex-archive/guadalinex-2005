#. Type: title
#. Description
#: ../templates:147 ../apt-setup-udeb.templates:3
msgid "Configure apt"
msgstr "Set opp apt"

#. Type: text
#. Description
#: ../templates:151
msgid "Select and install packages"
msgstr "Vel og installer pakkar"

#. Type: text
#. Description
#: ../templates:155
msgid "Configure the Mail Transfer Agent"
msgstr "Set opp overføringsagent for e-post"

#. Type: text
#. Description
#: ../templates:159
msgid "Finish configuring the base system"
msgstr "Ferdig med oppsettet av grunnsystemet"

#. Type: text
#. Description
#: ../templates:163
msgid "Exit the base system configuration"
msgstr "Avslutt oppsettet av grunnsystemet"

#. Type: text
#. Description
#: ../templates:167
msgid "Execute a shell"
msgstr "Køyr eit skal"

#. Type: text
#. Description
#: ../templates:171
#, fuzzy
msgid "Installing packages"
msgstr "Vel og installer pakkar"

#. Type: text
#. Description
#: ../templates:175
msgid "Preparing for installation..."
msgstr ""

#. Type: text
#. Description
#: ../templates:184
msgid "Registering documentation, please wait..."
msgstr ""

